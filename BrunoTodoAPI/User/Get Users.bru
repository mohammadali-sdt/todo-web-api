meta {
  name: Get Users
  type: http
  seq: 1
}

get {
  url: {{URL}}:{{PORT}}/api/users
  body: none
  auth: none
}

params:query {
  ~fields: username,email
  ~orderBy: age desc,name
  ~searchTerm: mr
  ~minAge: 28
  ~pageNumber: 1
  ~pageSize: 1
}

headers {
  Accept: application/vnd.vinix.hateoas+json
  ~Accept: text/csv
  ~Accept: text/xml
}
